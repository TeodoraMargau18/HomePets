<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banchelorapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fundalalbastrulogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fundalalbastrulogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.BanchelorApp">
        <activity android:name=".InfoAdoptie"></activity>
        <activity android:name=".ServiciiActivity" />
        <activity android:name=".SalonSelectatActivity" />
        <activity android:name=".ProfilMedicalActivity" />
        <activity android:name=".CentruAdoptiiActivity" />
        <activity android:name=".ListaSaloaneActivity" />
        <activity android:name=".AnimaleleMeleActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".AuthentificationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>